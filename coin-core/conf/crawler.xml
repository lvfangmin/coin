<?xml version="1.0" encoding="utf-8" ?>
<config>
    <targets>
        <target>
               <name>okcoin</name>
               <interval>60</interval>
               <type>btc</type>
               <url>https://www.okcoin.com/market.do?symbol=0</url>
               <latestPriceSelector>#marketLast</latestPriceSelector>
               <buyInSelector>div.real-left tbody tr</buyInSelector>
               <sellOutSelector>div.real-right tbody tr</sellOutSelector>
               <placeholderLength>1</placeholderLength>
        </target>
        <target>
               <name>okcoin</name>
               <interval>60</interval>
               <type>ltc</type>
               <url>https://www.okcoin.com/market.do?symbol=1</url>
               <latestPriceSelector>#marketLast</latestPriceSelector>
               <buyInSelector>div.real-left tbody tr</buyInSelector>
               <sellOutSelector>div.real-right tbody tr</sellOutSelector>
               <placeholderLength>1</placeholderLength>
        </target>
        <target>
               <name>yuanbaohui</name>
               <interval>60</interval>
               <type>ybc</type>
               <url>http://ybh.ybcoin.com</url>
               <latestPriceSelector>#btc_buyone</latestPriceSelector>
               <buyInSelector>div.realTableLeft tbody tr</buyInSelector>
               <sellOutSelector>div.realTableRight tbody tr</sellOutSelector>
               <placeholderLength>1</placeholderLength>
        </target>
    </targets>
</config>
